{
  "name": "flyt",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    },
    {
      "src": "src/api/server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/callback",
      "dest": "src/api/server.js",
      "headers": { "Access-Control-Allow-Origin": "*" }
    },
    {
      "src": "/policy",
      "dest": "src/api/server.js",
      "headers": { "Access-Control-Allow-Origin": "*" }
    },
    {
      "src": ".*",
      "dest": "public/index.html"
    }
  ]
}
