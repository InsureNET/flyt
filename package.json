{
  "name": "flyt",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "homepage": "/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "now-build": "npm run build && mv build dist",
    "deploy": "npm run deploy:contract && npm run deploy:dapp",
    "deploy:dapp": "now",
    "deploy:contract": "node ./scripts/deploy.js truffle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robin-thomas/flight.git"
  },
  "author": "Robin Thomas <robinthomas2591@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robin-thomas/flight/issues"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@truffle/hdwallet-provider": "1.0.22",
    "bootstrap": "^4.3.1",
    "chainlink": "^0.7.5",
    "date-fns": "^2.4.1",
    "express": "^4.17.1",
    "keyv": "^3.1.0",
    "lodash": "^4.17.15",
    "lzutf8": "^0.5.5",
    "mdbreact": "^4.21.0",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "react": "^16.10.2",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-datepicker": "^2.9.6",
    "react-dom": "^16.10.2",
    "react-scripts": "^3.2.0",
    "web3": "^1.2.1"
  },
  "devDependencies": {
    "eslint-plugin-node": "^10.0.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "react-app"
    ],
    "rules": {
      "no-constant-condition": [
        "off"
      ],
      "no-useless-catch": [
        "off"
      ],
      "node/no-missing-require": [
        "error"
      ],
      "node/no-missing-import": [
        "error"
      ],
      "node/no-unpublished-import": [
        "warn"
      ],
      "node/no-unpublished-require": [
        "warn"
      ],
      "node/no-unsupported-features/es-syntax": [
        "off"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
